<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd
        ">

    <!-- Import the real changelog -->
    <include file="../../classes/ddl/changelog-master.xml" relativeToChangelogFile="true"/>

    <changeSet id="load-test-data" author="klenkes74">
        <comment>Load test data for integration tests</comment>
        <loadData tableName="PROJECTS" file="../data/PROJECTS.csv" relativeToChangelogFile="true" encoding="UTF-8"
                  quotchar="&quot;" separator=";"/>
    </changeSet>
</databaseChangeLog>